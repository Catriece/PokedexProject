const h=Object.freeze({TOPCARD_PRIMARY:"TOPCARD_PRIMARY",TOPCARD_SECONDARY:"TOPCARD_SECONDARY",TOPCARD_SUPPLEMENTARY:"TOPCARD_SUPPLEMENTARY",TOPCARD_OVERFLOW:"TOPCARD_OVERFLOW",STICKY_HEADER:"STICKY_HEADER",BROWSEMAP:"BROWSEMAP",REUSABLE_SEARCH:"REUSABLE_SEARCH",PYMK:"PYMK",EVENT_SPEAKER:"EVENT_SPEAKER",EVENT_ATTENDEE:"EVENT_ATTENDEE"})
e.SUPPORTED_USE_CASES=h}))

